    {
      "parameters": {
        "jsCode": "const results = [];\nconst emails = $('Client Onboard Webhook').first().json.body.emails;\n\nfor (const item of $input.all()) {\n  const folderId = item.json.id;\n  const folderName = item.json.name;\n\n  for (const email of emails) {\n    results.push({\n      json: {\n        folderId,\n        folderName,\n        email,\n        role: \"writer\"\n      }\n    });\n  }\n}\n\nreturn results;\n"
      },
